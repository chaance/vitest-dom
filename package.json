{
  "name": "vitest-dom",
  "version": "0.1.1",
  "description": "Custom Vitest matchers to test the state of the DOM, forked from jest-dom.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaance/vitest-dom.git"
  },
  "license": "MIT",
  "author": "Chance Stricklad <hi@chance.dev>",
  "type": "module",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "extend-expect.d.ts",
    "extend-expect.js",
    "matchers.d.ts",
    "matchers.js"
  ],
  "scripts": {
    "build": "tsup",
    "test:dom": "vitest --config vitest.config.dom.ts",
    "test:node": "vitest --config vitest.config.node.js",
    "test": "run-p test:*",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{ts,js}'"
  },
  "dependencies": {
    "aria-query": "^5.3.0",
    "chalk": "^5.3.0",
    "css.escape": "^1.5.1",
    "dom-accessibility-api": "^0.6.1",
    "lodash-es": "^4.17.21",
    "redent": "^4.0.0"
  },
  "devDependencies": {
    "@types/aria-query": "^5.0.1",
    "@types/css.escape": "^1.5.0",
    "@types/lodash-es": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "esbuild": "^0.14.48",
    "eslint": "^8.49.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "fast-glob": "^3.3.1",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "pretty-format": "^28.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  },
  "peerDependencies": {
    "vitest": ">=0.31.0"
  }
}
